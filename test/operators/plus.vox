print(123 + 456); // expect: 579
print("str" + "ing"); // expect: string

// Mixed string conversions.
print(123 + "int"); // expect: 123int
print("str" + 456); // expect: str456
print("str" + null); // expect: strnull
print(null + "ing"); // expect: nulling
print("str" + true); // expect: strtrue
print(false + "ing"); // expect: falseing

